{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "variable-name": [false],
    "no-console": [false],
    "ordered-imports": [false],
    "no-unused-variable": [true, {"ignore-pattern": "^_"}],
    "jsx-no-lambda": [false],
    "no-unused-expression": false
  }
}
